<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CBCDCD"
    tools:context=".Admin">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="405dp"
        android:layout_height="474dp"
        android:layout_marginBottom="43dp"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_26_2260470_transparent_admin_icon_png_admin_logo_png_png" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="17dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="16dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2">

        <Button
            android:id="@+id/button"
            android:layout_width="153dp"
            android:layout_height="92dp"
            android:layout_marginStart="20dp"
            android:backgroundTint="#C57575"
            android:text="Kelime İşlemleri" />

        <Button
            android:id="@+id/button4"
            android:layout_width="153dp"
            android:layout_height="92dp"
            android:layout_marginStart="20dp"
            android:backgroundTint="#C57575"
            android:text="İstek-Şikayet Görüntüleme" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>